                "temp_corr_growth_b": "-p*k*(T-T_opt)**2*b_daysy",
                "temp_corr_competition_b": "-k*(T-T_opt)**2*w_daysy*b_daysy",
                "temp_corr_carrying_b": "-k*(T-T_opt)**2*b_daysy*b_daysy",
                "decay_b": "-gamma*b_daysy"
            },
            "initial_value": 0.2
        }
    },
    "parameters": {
